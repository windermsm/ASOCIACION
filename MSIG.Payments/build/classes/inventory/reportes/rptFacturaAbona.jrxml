	<queryString>
		<![CDATA[select 	'' as codigo,
	'C' as tipo, 
	'S/N' as numero_contador,
	'0'as numero_derecho,
	a.nombre_cliente_abona as nombre,
	a.direccion_cliente_abona as direccion,
	'Escuintla' as lugar,
	a.nit_cliente_abona as nit,
  b.saldo_credito as saldo,
	'' as saldo_varios,
	'' as iva_total,
	a.motivo_abona as observaciones,
	a.monto_abona as total,
	'' as lectura_anterior,
	'' as lectura_nueva,
	'' as fecha_lectura,
	'' as volumen_consumido,
	'' as costo_m3,
	'' as fecha_vence
from  abona as a,
      credito as b
where a.id_credito = b.id_credito  
and serie_factura_abona = $P{P_SERIE} and numero_factura_abona = $P{P_NO_FACTURA}]]>
	</queryString>

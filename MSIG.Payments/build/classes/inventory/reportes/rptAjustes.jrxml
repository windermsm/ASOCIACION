	<queryString>
		<![CDATA[select  a.anio_ajuste as anio,
if(a.mes_ajuste=1,'Enero',if(a.mes_ajuste=2,'Febrero',if(a.mes_ajuste=3,'Marzo',if(a.mes_ajuste=4,'Abril',if(a.mes_ajuste=5,'Mayo',if(a.mes_ajuste=6,'Junio',if(a.mes_ajuste=7,'Julio',if(a.mes_ajuste=8,'Agosto',if(a.mes_ajuste=9,'Septiembre',if(a.mes_ajuste=10,'Octubre',if(a.mes_ajuste=11,'Noviembre',if(a.mes_ajuste=12,'Diciembre','')))))))))))) as nombre_mes,
        c.id_cliente as codigo,
        c.nombre_cliente as cliente,
        date_format(a.fecha_ajuste,'%d-%m-%Y') as fecha,
        round(a.saldo_ajuste, 2) as ajuste,
        a.nota_ajuste as nota
from    ajuste as a,
        suscripcion as b,
        cliente as c
where   a.id_cobro = b.id_suscripcion
    and b.id_cliente = c.id_cliente
    and a.mes_ajuste = $P{P_MES}
    and a.anio_ajuste = $P{P_ANIO}
    and c.colonia_cliente = $P{P_COLONIA}]]>
	</queryString>

	<queryString>
		<![CDATA[select * from (
select  *
from    ingreso
where   date_format(fecha_factura_ingreso, '%Y-%m-%d') between $P{P_FECHA_INICIAL} and $P{P_FECHA_FINAL}
and     estado_ingreso = 'Activo'

union all 

select  id_abona as id_ingreso,
        serie_factura_abona as serie_factura_ingreso,
        numero_factura_abona as no_factura_ingreso,
        nit_cliente_abona as nit_cliente_ingreso,
        nombre_cliente_abona as nombre_cliente_ingreso,
        direccion_cliente_abona as direccion_ingreso,
        fecha_factura_abona as fecha_factura_ingreso,
        monto_abona as monto_ingreso,
        'Contadores al credito' as cuenta_ingreso,
        motivo_abona as motivo_ingreso,
        estado_abona as estado_ingreso,
        fecha_abona as fecha_ingreso
from    abona
where   date_format(fecha_factura_abona, '%Y-%m-%d') between $P{P_FECHA_INICIAL} and $P{P_FECHA_FINAL}
and     estado_abona = 'Activo'
) as x
order by 7,3,2]]>
	</queryString>

	<queryString>
		<![CDATA[select  a.anio_abono as anio,
if(a.mes_abono=1,'Enero',if(a.mes_abono=2,'Febrero',if(a.mes_abono=3,'Marzo',if(a.mes_abono=4,'Abril',if(a.mes_abono=5,'Mayo',if(a.mes_abono=6,'Junio',if(a.mes_abono=7,'Julio',if(a.mes_abono=8,'Agosto',if(a.mes_abono=9,'Septiembre',if(a.mes_abono=10,'Octubre',if(a.mes_abono=11,'Noviembre',if(a.mes_abono=12,'Diciembre','')))))))))))) as nombre_mes,
	a.factura_abono as factura,
        c.id_cliente as codigo,
        c.nombre_cliente as cliente,
        a.fecha_abono as fecha,
        round(a.pago_abono + a.saldo_abono, 2) as saldo_anterior,
        round(a.pago_abono, 2) as abono,
        round(a.saldo_abono, 2) as saldo_actual
from    abono as a,
        suscripcion as b,
        cliente as c
where   a.id_cobro = b.id_suscripcion
    and b.id_cliente = c.id_cliente
    and a.mes_abono = $P{P_MES}
    and a.anio_abono = $P{P_ANIO}
    and c.colonia_cliente = $P{P_COLONIA}
order by a.factura_abono]]>
	</queryString>

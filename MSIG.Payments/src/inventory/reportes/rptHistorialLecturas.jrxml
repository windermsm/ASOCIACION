	<queryString>
		<![CDATA[select  c.nombre_cliente as cliente,
        a.id_suscripcion as suscripcion,
        a.mes_lectura as mes,
        a.anio_lectura as anio,
        a.anterior_lectura as anterior,
        a.nueva_lectura as nueva
from    lectura as a,
        suscripcion as b,
        cliente as c
where   a.anio_lectura = $P{P_ANIO}
    and a.estado_lectura = 'A'
    and a.id_suscripcion = b.id_suscripcion
    and b.id_cliente = c.id_cliente
    AND b.id_cliente = ifnull($P{P_CLIENTE}, b.id_cliente)
    AND b.id_servicio = ifnull($P{P_SERVICIO}, b.id_servicio) 
order by a.id_suscripcion, a.anio_lectura ,a.mes_lectura]]>
	</queryString>
